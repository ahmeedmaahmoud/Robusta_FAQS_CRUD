<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceBlock name="page.title">
        <action method="setPageTitle">
            <argument name="title" xsi:type="string">Categories</argument>
        </action>
    </referenceBlock>

    <body>
        <referenceContainer name="content">

            <block class="Myvendor\Faqs\Block\Adminhtml\CategoryIndex" template="Myvendor_Faqs::category.phtml"/>
        </referenceContainer>

        <referenceBlock name="content">
            <block class="Myvendor\Faqs\Block\Adminhtml\CategoryAddButton" name="category.add.button" template="Myvendor_Faqs::category_add_button.phtml" before="-"/>
        </referenceBlock>
    </body>
</page>
